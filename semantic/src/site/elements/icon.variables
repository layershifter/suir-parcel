/*******************************
    User Variable Overrides
*******************************/

@src:
        url("@{fontPath}/@{fontName}.woff") format('woff'),
        url("@{fontPath}/@{fontName}.ttf") format('truetype'),
        url("@{fontPath}/@{fontName}.svg#icons") format('svg')
;
@fallbackSRC: unset;

@importBrandIcons: false;
@importOutlineIcons: false;